{
  "name": "schokotron",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "author": "David Beermann <mail@davidbeermann.com>",
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "cp:socket":
      "cp ./node_modules/socket.io-client/dist/socket.io.js ./public/vendor",
    "cp:socket:map":
      "cp ./node_modules/socket.io-client/dist/socket.io.js.map ./public/vendor",
    "mkdir:vendor": "mkdir -p ./public/vendor",
    "prestart":
      "yarn install && yarn run mkdir:vendor && yarn run cp:socket && yarn run cp:socket:map",
    "start": "./run"
  },
  "dependencies": {
    "express": "^4.16.2",
    "ramda": "^0.25.0",
    "socket.io": "^2.0.4",
    "twit": "^2.2.9"
  },
  "devDependencies": {
    "jest": "^21.2.1"
  }
}
